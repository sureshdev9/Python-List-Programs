        "# Python program to count positive and negative numbers in a list\n",
        "\n",
        "a = [10, -20, 30, -40, 50, -60, 0]\n",
        "\n",
        "p =0\n",
        "n =0\n",
        "\n",
        "for i in a:\n",
        "  if i>0:\n",
        "    p+=1\n",
        "  elif i<0:\n",
        "    n+=1\n",
        "\n",
        "print(p,n)\n",
        "\n",
        "p = len(list(filter(lambda x : x>0,a)))\n",
        "n = len(list(filter(lambda x : x<0,a)))\n",
        "print(p,n)\n"
