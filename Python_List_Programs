      "cell_type": "code",
      "source": [
        "# Remove Multiple Elements from List in Python\n",
        "\n",
        "a = [10, 20, 30, 40, 50, 60, 70]\n",
        "\n",
        "# Elements to remove\n",
        "target = [20, 40, 60]\n",
        "\n",
        "for i in a:\n",
        "  if i  in target:\n",
        "    (a.remove(i))\n",
        "print(a)\n",
        "\n",
        "a = [ i for i in a if i not in target]\n",
        "print(a)\n",
        "\n",
        "\n",
        "for i in a:\n",
        "  if i in target:\n",
        "    print(i)\n",
        "a = list(filter(lambda x :x not in target,a))\n",
        "print(a)\n",
        "\n"
