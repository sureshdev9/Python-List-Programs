        "from functools import reduce\n",
        "from operator import mul\n",
        "# Multiply All Numbers in the List in Python\n",
        "\n",
        "lst  =[2,4,6,8]\n",
        "mul1=1\n",
        "for i in lst:\n",
        "  mul1 *= i\n",
        "  print(mul1)\n",
        "print(mul1)\n",
        "\n",
        "\n",
        "\n",
        "resp = reduce(lambda x,y:x*y,lst)\n",
        "print(resp)\n",
        "resp = reduce(mul,lst)\n",
        "print(resp)"
